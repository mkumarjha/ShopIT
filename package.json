{
  "name": "shopit",
  "version": "1.0.0",
  "description": "ecommerce website",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "export NODE_ENV=DEVELOPMENT && nodemon backend/server",
    "prod": "export NODE_ENV=PRODUCTION && nodemon backend/server",
    "seeder": "node backend/utils/seeder.js",
    "data:destroy": "node backend/utils/seeder -d",
    "render-postbuild": "NPM_CONFIG_PRODUCTION=false && npm install --prefix frontend && npm --prefix frontend run build"
  },
  "author": "Mithilesh Jha",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "cloudinary": "^1.33.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.8.3",
    "nodemailer": "^6.8.0",
    "stripe": "^11.7.0",
    "validator": "^13.7.0",
    "nodemon": "^2.0.20"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
